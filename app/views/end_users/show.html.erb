<div class="end_users-show">
	<div class="end-user-info">
		<div class="end_users-table">
			<table class="table table-striped">
				<tr>
					<td class="end_users-table-title" colspan="2">マイページ</td>
				</tr>
				<tr>
					<td>名前</td>
					<td><%=@end_user.first_name%> <%=@end_user.last_name%></td>
				</tr>
				<tr>
					<td>カナ</td>
					<td><%=@end_user.kana_first%> <%=@end_user.kana_last%></td>
				</tr>
				<tr>
					<td>郵便番号</td>
					<td><%=@end_user.zip%></td>
				</tr>
				<tr>
					<td>住所</td>
					<td><%=@end_user.address%></td>
				</tr>
				<tr>
					<td>電話番号</td>
					<td><%=@end_user.phone_number%></td>
				</tr>
				<tr>
					<td>メールアドレス</td>
					<td><%=@end_user.email%></td>
				</tr>
				<tr>
					<td>配送先住所</td>
					<td><%=%></td>
				</tr>
			</table>
		</div>
		<div class="end_user-show-edit-btns">
      <%=link_to end_user_order_path(@end_user) do %><div class="btn btn-info">購入履歴</div><% end %>
      <%=link_to edit_end_user_path(@end_user) do %><div class="btn btn-success">アカウント情報の変更</div><% end %>
      <%=link_to end_user_leave_path(@end_user), method: :delete, data: {confirm: '本当に退会しますか？'} do %><div class="btn btn-danger">退会</div><% end %>
		</div>
	</div>
</div>
