<%= render "layouts/admin_header"%>

<div>
	<div>
		<button class="btn btn-block"><%= link_to "商品追加", admins_sell_cds_main_new_path %></button>
	</div>




<% @sell_cds.reverse_each do |sell_cd| %>
	<table>
				<tr><td><%= attachment_image_tag sell_cd, :image, format: 'jpeg' , size: "50x50" %></td></tr>
				<tr><td><%= link_to sell_cd.title , admins_sell_cd_path(sell_cd) %></td></tr>
				<tr>
					<td><%= sell_cd.artist.artist %></td>
				</tr>
				<tr><td><%= sell_cd.stock %></td></tr>
				<tr><td><span>sell_cd.value</span><span>sell_cd.sale_status</span></td></tr>
				<tr><td><span><%= link_to "編集",  admins_sell_cds_main_edit_path(sell_cd.id) %></span> / <span><%= link_to "削除",admins_sell_cd_path(sell_cd.id),method: :delete, data: { confirm: '本当に削除しますか？'} %></span></td></tr>

        </table>
	<%= paginate @sell_cds %>
	<% end %>


</div>